document.addEventListener("DOMContentLoaded", function () {
  console.log("★");
  const header = document.querySelector("header");
  const htmlStr = `
  <nav>
    <h1>学生管理システム</h1>
    <a href="index.html">TOP</a>
    <a href="students.html">学生一覧</a>
    <a href="courses.html">コース一覧</a>
  </nav>
  `;
  header.innerHTML += htmlStr;
});

